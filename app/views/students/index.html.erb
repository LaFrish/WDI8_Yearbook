<div class="page-content-wrapper">
  <h1 class="page-header">Student Index</h1>

  <button><%= link_to "Add Student", new_student_path(@student)%></h2></button>


  <% @students.each do |student| %>
<button><%= link_to student.name, student%></button>
  <% end %>

  <ul>
    <li>
      <% @students.each do |student| %>
<span class="index_img">    <%= link_to image_tag(student.img_url), student_path(student) %></span>
  </li>
</ul>

  <% end %>
